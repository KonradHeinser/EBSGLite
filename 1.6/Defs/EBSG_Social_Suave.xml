<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <GeneDef ParentName="EBSG_Social_Suave">
        <defName>EBSG_Boorish</defName>
        <label>boorish</label>
        <description>Carriers of this gene are innately boorish, making it more likely that others will be completely uninterested in whatever the pawn is talking about.</description>
        <iconPath>EBSG/Gene_Boorish</iconPath>
        <displayOrderInCategory>102.4</displayOrderInCategory>
        <biostatMet>2</biostatMet>
        <statFactors>
            <SocialImpact>0.70</SocialImpact>
            <TradePriceImprovement>0.70</TradePriceImprovement>
        </statFactors>
        <symbolPack>
            <prefixSymbols>
                <li><symbol>boor</symbol></li>
            </prefixSymbols>
        </symbolPack>
    </GeneDef>

    <GeneDef ParentName="EBSG_Social_Suave">
        <defName>EBSG_Suave</defName>
        <label>suave</label>
        <description>Carriers of this gene are innately suave. This helps them keep others interested in what you're talking about.</description>
        <iconPath>EBSG/Gene_Suave</iconPath>
        <displayOrderInCategory>102.7</displayOrderInCategory>
        <biostatMet>-2</biostatMet>
        <statFactors>
            <SocialImpact>1.3</SocialImpact>
            <TradePriceImprovement>1.2</TradePriceImprovement>
        </statFactors>
        <symbolPack>
            <prefixSymbols>
                <li><symbol>sua</symbol></li>
            </prefixSymbols>
        </symbolPack>
    </GeneDef>

    <GeneDef ParentName="EBSG_Social_Suave">
        <defName>EBSG_ExtremelySuave</defName>
        <label>extremely suave</label>
        <description>Carriers of this gene are innately extremely suave. This helps them keep others interested in what you're talking about.</description>
        <iconPath>EBSG/Gene_ExtremelySuave</iconPath>
        <displayOrderInCategory>102.9</displayOrderInCategory>
        <biostatMet>-4</biostatMet>
        <statFactors>
            <SocialImpact>1.6</SocialImpact>
            <TradePriceImprovement>1.4</TradePriceImprovement>
        </statFactors>
        <symbolPack>
            <prefixSymbols>
                <li><symbol>sua</symbol></li>
            </prefixSymbols>
        </symbolPack>
    </GeneDef>
</Defs>